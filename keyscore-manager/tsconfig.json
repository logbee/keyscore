{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@keyscore-manager-models/*": [
        "../modules/keyscore-manager-models/*"
      ],
      "@keyscore-manager-material/*": [
        "../modules/keyscore-manager-material/*"
      ],
      "@keyscore-manager-pipeline-parameters/*": [
        "../modules/keyscore-manager-pipeline-parameters/*"
      ],
      "@keyscore-manager-test-fixtures/*": [
        "../modules/keyscore-manager-test-fixtures/*"
      ],
      "@keyscore-manager-rest-api/*": [
        "../modules/keyscore-manager-rest-api/*"
      ]
    },
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2017",
      "dom",
      "es2016",
      "es5",
      "es6"
    ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "strict": false,
    "strictFunctionTypes": false,
    "pretty": true,
    "resolveJsonModule": true
  },
  "types": [
    "node"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
