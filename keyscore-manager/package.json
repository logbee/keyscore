      "^@keyscore-manager-models/(.*)": "<rootDir>/modules/keyscore-manager-models/$1",
      "^@keyscore-manager-material/(.*)": "<rootDir>/modules/keyscore-manager-material/$1",
      "^@keyscore-manager-pipeline-parameters/(.*)": "<rootDir>/modules/keyscore-manager-pipeline-parameters/$1",
      "^@keyscore-manager-test-fixtures/(.*)": "<rootDir>/modules/keyscore-manager-test-fixtures/$1",
      "^@keyscore-manager-rest-api/(.*)": "<rootDir>/modules/keyscore-manager-rest-api/$1",
      "^@keyscore-manager-data-preview/(.*)": "<rootDir>/modules/keyscore-manager-data-preview/$1"
    }
  }
